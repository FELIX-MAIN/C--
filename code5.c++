#include <iostream>
using namespace std;

int main() {
    // Declare and initialize variables of different data types
    int num1 = 10;
    float num2 = 3.14;
    char character = 'A';
    bool flag = true;

    // Print the values of the variables
    cout << "Integer: " << num1 << endl;
    cout << "Float: " << num2 << endl;
    cout << "Character: " << character << endl;
    cout << "Boolean: " << flag << endl;

    return 0;
}
